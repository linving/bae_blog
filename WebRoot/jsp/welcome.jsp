<script>
  var currentIndex = 1;
  var moving = false;
  var frontActive = true;
  
  function setIndex(index, carousel) {
    if(index != currentIndex && !moving) {
      moving = true
      currentIndex = index
      carousel.scroll(index)
      setActiveScreenshot(index)
      $("#line").hide();
    }
  }
  
  function bindPagenum(carousel) {
    $('a.pagenum').bind('click', function() {
        var index = $.jcarousel.intval($(this).attr("rel"));
        setIndex(index + 1, carousel)
        return false
    });
  
    $('.callout').bind('click', function() {
        var index = $.jcarousel.intval($(this).attr("rel"));
        setIndex(index + 1, carousel)
        return false
    });
  }
  function setActivePagenum(num) {
    $(".pagenum").each(function(index) {
      if(index === num - 1) {
        $(this).addClass("active");
      } else {
        $(this).removeClass("active");
      }
    });
  };
  function setActiveCallout(num) {
    $(".callout").each(function(index) {
      if(index === num - 1) {
        $(this).addClass("active");
      } else {
        $(this).removeClass("active");
      }
    });
  };
  
  function setActiveScreenshot(num) {
    url = "url(/images/index/screenshot_" + num + ".jpg)"
  
    if (frontActive) {
      active = "#front"         
      buffer = "#back"
    } else {
      buffer = "#front"
      active = "#back"  
    }
  
    frontActive = !frontActive
    
    $(".interface_container"+active).fadeOut(1200)    
    $(".interface_container"+buffer).css("background", url)
    $(".interface_container"+buffer).fadeIn(1200)
    //background: url("/imgs/screen1.png");
  }
  
  function onItemIn(carousel, item, idx, state) {
    moving = false;
    setActivePagenum(idx);
    setActiveCallout(idx);
    url = "url(/images/index/line_" + idx + ".png)"
    $("#line").css("background", url)
    $("#line").show();
  };
  
  function onItemOut(carousel, item, idx, state) {
  };
  
  $(document).ready(function() {
    $('#slides').jcarousel({
      vertical: true,
      scroll: 1,
      animation: 800,
      initCallback: bindPagenum,
      // auto: 1,
      itemFirstInCallback: onItemIn,
      itemFirstOutCallback: onItemOut,
      buttonNextHTML: null,
      buttonPrevHTML: null,
      //itemFallbackDimension: 300
    });
  });
</script>

